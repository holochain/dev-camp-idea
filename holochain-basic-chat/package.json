    "hc:build-linux": "mkdir -p dna && cd dna-src && ../linux/hc package --strip-meta -o ../dna/holo-chat.hcpkg",
    "hc:start-linux": "../linux/holochain -c ./conductor-config.toml",
    "hc:build-win": "mkdir -p dna && cd dna-src && ../win/hc package --strip-meta -o ../dna/holo-chat.hcpkg",
    "hc:start-win": "../win/holochain -c ./conductor-config.toml"
